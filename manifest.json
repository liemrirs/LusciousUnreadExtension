{
  "manifest_version": 2,

  "name": "Luscious Message Checker",
  "description": "This extension shows the unread count of messages and notifications",
  "version": "1.3",
  "background": { "scripts": ["jquery.min.js","background.js"] },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "http://luscious.net/",
    "https://luscious.net/"
  ]
}
